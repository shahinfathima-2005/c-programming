#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    
    int a,i;
    scanf("%d",&a);
    if (a>=1 && a<=9)
    {
    for (i=1; i<= a;i++)
    {
    printf("%d x %d = %d\n",a,i,a*i);
    }
    } else
    {
    printf("Invalid Input");
    }
     return 0;
  }

   
