#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n; 
    int i;
    int sum=0;
    scanf("%d",&n); 
    int arr[n]; 
    for (i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    } 
    for (i=0;i<n;i++)
    {
        if (arr[i]>0)
        {
            sum+=arr[i]*arr[i];
        }
    }
    printf("%d\n",sum);

    return 0;
}
  
