#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b,c,d;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    scanf("%d",&d);
    printf("Items Details\n");
    printf("A4sheet:%d\n",a);
    printf("Pen:%d\n",b); 
    printf("Pencil:%d\n",c);
    printf("Eraser:%d",d);
    return 0;
}
