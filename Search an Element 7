#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  
    int n,i,count,found=0;
    scanf("%d",&n);
    int arr[n];   
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }  
    scanf("%d",&count);  
    for(i=0;i<n;i++)
    {
        if(arr[i]==count)
        {
            found = 1;
            break;
        }
    }
    if(found)
        printf("%d is presented in an array.",count);
    else
        printf("%d is not presented in an array.",count);

    return 0;
}
